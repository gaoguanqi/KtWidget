<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="DotView">
        <attr name="dotColor" format="color|reference"/>
        <attr name="dotStyle" format="enum">
            <enum name="FILL" value="0"/>
            <enum name="STROKE" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ImageTextLayout">
        <attr name="textContent" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color|reference"/>
        <attr name="imageIcon" format="reference"/>
    </declare-styleable>
</resources>